    "version": "0.2.0",
    "configurations": [
        {
            "name": "GW Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}", // Command to pick the process
            // "program": "C:/Program Files (x86)/Guild Wars/Gw.exe",
            // "MIMode": "gdb",
            // "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
            // "setupCommands": [
            //     {
            //         "description": "Enable pretty-printing for gdb",
            //         "text": "-enable-pretty-printing",
            //         "ignoreFailures": true
            //     }
            // ]
            "logging": {
                "programOutput": true,
            }
        },
        {
            "name": "Debug Injector",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/dev/Debug/HerosInsight/Launch_HerosInsight.exe",
            "cwd": "${workspaceFolder}/build/dev/Debug/HerosInsight",
        }
    ]
}